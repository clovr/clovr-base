[clovr_metagenomics_prep default]
$;INPUT_FILE_LIST$;=${INPUT_FILE_LIST}
$;MAPPING_FILE$;=${MAPPING_FILE}

[split_multifasta uclust]
$;TOTAL_FILES$;= ${TOTAL_FILES}
$;GROUP_COUNT$; = ${GROUP_COUNT}

[uclust default]
$;GROUP_COUNT$; = ${GROUP_COUNT}

[split_multifasta blast]
$;TOTAL_FILES$;= ${TOTAL_FILES}
$;GROUP_COUNT$; = ${GROUP_COUNT}

[ncbi-blastx default]
$;DATABASE_PATH$;=/mnt/upload/COGdb.fasta
$;GROUP_COUNT$; = ${GROUP_COUNT}
$;OTHER_OPTS$;=-m 8

[ncbi-blastn default]
$;DATABASE_PATH$;=/mnt/upload/RefSeqdb
$;GROUP_COUNT$; = ${GROUP_COUNT}
$;OTHER_OPTS$;=-W 11 -m 8

[clovr_metagenomics_tables functional]
$;MAPPING_FILE$;=${MAPPING_FILE}
$;ANNOTATION_FILE$;=/mnt/upload/COGdb.antn

[clovr_metagenomics_tables taxonomy]
$;MAPPING_FILE$;=${MAPPING_FILE}
$;ANNOTATION_FILE$;=/mnt/upload/RefSeqdb.antn  

[metastats functional]
$;GROUP_COUNT$; = ${GROUP_COUNT}

[metastats taxonomy]
$;GROUP_COUNT$; = ${GROUP_COUNT}

[skiff functional]
$;GROUP_COUNT$; = ${GROUP_COUNT}

[skiff taxonomy]
$;GROUP_COUNT$; = ${GROUP_COUNT}

[piecharts taxonomy]
$;GROUP_COUNT$; = ${GROUP_COUNT}

[piecharts functional]
$;GROUP_COUNT$; = ${GROUP_COUNT}

