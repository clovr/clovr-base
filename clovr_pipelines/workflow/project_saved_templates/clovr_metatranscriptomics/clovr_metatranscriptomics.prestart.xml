<?xml version="1.0" encoding="UTF-8"?>
<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				xsi:schemaLocation='commandSet.xsd'>
  <commandSet type="serial">
    <state>incomplete</state>
    <name>total_metagenomics_stub.prestart</name>
    <command>
      <name>Fasta-file Check</name>
      <type>RunUnixCommand</type>
      <executable>/opt/vappio-py/vappio/cli/tagData.py</executable>
      <arg>--name local --tag-name $;FASTA_TAG$; --overwrite --recursive $;FASTA_FILES$;</arg>
    </command>
    <command>
      <name>Map-file Check</name>
      <type>RunUnixCommand</type>
      <executable>vp-describe-task</executable>
      <arg>--block --exit-code `vp-run-metrics -t -c input.INPUT_TAG=$;MAPPING_TAG$; "tag-is-meta"`</arg>
    </command>
  </commandSet>
</commandSetRoot>
