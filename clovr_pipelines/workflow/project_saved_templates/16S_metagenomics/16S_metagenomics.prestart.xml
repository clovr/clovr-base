<?xml version="1.0" encoding="UTF-8"?>
<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				xsi:schemaLocation='commandSet.xsd'>
  <commandSet type="serial">
    <state>incomplete</state>
    <name>16S_metagenomics_stub.prestart</name>
    <command>
      <name>Touch files</name>
      <type>RunUnixCommand</type>
      <executable>touch</executable>
      <arg>$;FASTA_FILES$; $;OLIGOS_FILE$; $;QUALITY_FILE$; $;META_FILE$; $;DB_PATH$;</arg>
    </command>
    <command>
      <name>Tag fasta data</name>
      <type>RunUnixCommand</type>
      <executable>/opt/vappio-py/vappio/cli/tagData.py</executable>
      <arg>--name local --tag-name $;FASTA_TAG$; --overwrite --recursive $;FASTA_FILES$;</arg>
    </command>
    <command>
      <name>Tag oligos data</name>
      <type>RunUnixCommand</type>
      <executable>/opt/vappio-py/vappio/cli/tagData.py</executable>
      <arg>--name local --tag-name $;OLIGOS_TAG$; --overwrite --recursive $;OLIGOS_FILE$;</arg>
    </command>
    <command>
      <name>Tag quality data</name>
      <type>RunUnixCommand</type>
      <executable>/opt/vappio-py/vappio/cli/tagData.py</executable>
      <arg>--name local --tag-name $;QUALITY_TAG$; --overwrite --recursive $;QUALITY_FILE$;</arg>
    </command>
    <command>
      <name>Tag meta data</name>
      <type>RunUnixCommand</type>
      <executable>/opt/vappio-py/vappio/cli/tagData.py</executable>
      <arg>--name local --tag-name $;META_TAG$; --overwrite --recursive $;META_FILE$;</arg>
    </command>
        <command>
      <name>Tag 16S db data</name>
      <type>RunUnixCommand</type>
      <executable>/opt/vappio-py/vappio/cli/tagData.py</executable>
      <arg>--name local --tag-name $;DB_TAG$; --overwrite --recursive $;DB_PATH$;</arg>
    </command>
  </commandSet>
</commandSetRoot>
