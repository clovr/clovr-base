[ber post_overlap_analysis]
$;GROUP_COUNT$; = ${input.GROUP_COUNT}
$;PROJECT_ABBREVIATION$; = ${input.OUTPUT_PREFIX}
$;PROTEIN_DB$; = ${input.DATABASE_PATH}/db/uniref100/uniref100.fasta

