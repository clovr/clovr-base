<?xml version="1.0" encoding="UTF-8"?>
<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				xsi:schemaLocation='commandSet.xsd'>
  <commandSet type="serial">
    <state>incomplete</state>
    <name>16S_qiime.prestart</name>
    <command>
      <name>Touch files</name>
      <type>RunUnixCommand</type>
      <executable>touch</executable>
      <arg>$;FASTA_FILES$; $;META_FILE$;</arg>
    </command>
    <command>
      <name>Tag fasta data</name>
      <type>RunUnixCommand</type>
      <executable>/opt/vappio-py/vappio/cli/tagData.py</executable>
      <arg>--name local --tag-name $;FASTA_TAG$; --overwrite --recursive $;FASTA_FILES$;</arg>
    </command>
    <command>
      <name>Tag meta data</name>
      <type>RunUnixCommand</type>
      <executable>/opt/vappio-py/vappio/cli/tagData.py</executable>
      <arg>--name local --tag-name $;META_TAG$; --overwrite --recursive $;META_FILE$;</arg>
    </command>
  </commandSet>
</commandSetRoot>
