<?xml version="1.0" encoding="UTF-8"?>
<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				xsi:schemaLocation='commandSet.xsd'>
  <commandSet type="serial">
    <state>incomplete</state>
    <name>CloVR 16S Prestart</name>
    <command>
      <name>Fasta-or-fastq-file Check</name>
      <type>RunUnixCommand</type>
      <executable>vp-describe-task</executable>
      <arg>--show-all --block --exit-code `vp-run-metrics -t --pipeline-name $;PIPELINE_WRAPPER_NAME$; "filter-keys input.REFERENCE_FASTA input.FASTQ_TAG | tag-is-fasta-or-fastq"`</arg>
  </commandSet>
</commandSetRoot>
