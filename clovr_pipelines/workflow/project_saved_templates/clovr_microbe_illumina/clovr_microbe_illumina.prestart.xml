<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				xsi:schemaLocation='commandSet.xsd'>
  <commandSet type="serial">
    <state>incomplete</state>
    <name>clovr_microbe_illumina.prestart</name>
    <command>
      <name>Fasta-or-fastq-file Check</name>
      <type>RunUnixCommand</type>
      <executable>vp-describe-task</executable>
      <arg>--block --exit-code `vp-run-metrics -t --pipeline-name $;PIPELINE_WRAPPER_NAME$; "filter-keys input.SHORT_PAIRED_FILES input.LONG_PAIRED_FILES input.LONG_FILES input.SHORT_FILES | tag-is-fasta-or-fastq"</arg>
    </command>
  </commandSet>
</commandSetRoot>
