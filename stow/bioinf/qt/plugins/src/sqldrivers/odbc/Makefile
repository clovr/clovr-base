####### Build rules

all: Makefile  ../../../sqldrivers/$(TARGET)

../../../sqldrivers/$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC) $(SUBLIBS) $(OBJCOMP)  
	test -d ../../../sqldrivers/ || mkdir -p ../../../sqldrivers/
	-$(DEL_FILE) $(TARGET)
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS) $(OBJCOMP)
	-$(MOVE) $(TARGET) ../../../sqldrivers/



mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd /usr/local/bioinf/qt/src/moc && $(MAKE) )

Makefile: odbc.pro ../../../../.qmake.cache /usr/local/bioinf/qt/mkspecs/linux-g++/qmake.conf ../../../../lib/libqt.prl
	$(QMAKE) -spec /usr/local/bioinf/qt/mkspecs/linux-g++ -o Makefile odbc.pro
qmake: 
	@$(QMAKE) -spec /usr/local/bioinf/qt/mkspecs/linux-g++ -o Makefile odbc.pro

dist: 
	@mkdir -p .obj/qsqlodbc && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/qsqlodbc/ && ( cd `dirname .obj/qsqlodbc` && $(TAR) qsqlodbc.tar qsqlodbc && $(GZIP) qsqlodbc.tar ) && $(MOVE) `dirname .obj/qsqlodbc`/qsqlodbc.tar.gz . && $(DEL_FILE) -r .obj/qsqlodbc

