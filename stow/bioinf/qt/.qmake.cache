CONFIG +=  nocrosscompiler minimal-config small-config medium-config large-config full-config styles tools kernel widgets dialogs iconview workspace network canvas table xml sql release dll largefile stl ipv6 png no-gif zlib nis bigcodecs x11sm xkb dylib create_prl link_prl
QMAKESPEC = linux-g++
QT_SOURCE_TREE = /usr/local/bioinf/qt
QT_BUILD_TREE = /usr/local/bioinf/qt
QMAKE_ABSOLUTE_SOURCE_ROOT = $$QT_SOURCE_TREE
QT_PRODUCT=qt-free
styles += windows motif mac platinum sgi cde motifplus
kbd-drivers += tty
mouse-drivers += pc
QMAKE_MOC=/usr/local/bioinf/qt/bin/moc
QMAKE_UIC=/usr/local/bioinf/qt/bin/uic -L $$QT_BUILD_TREE/plugins
QMAKE_QMAKE=/usr/local/bioinf/qt/bin/qmake
QMAKE_MOC_SRC=/usr/local/bioinf/qt/src/moc
QMAKE_INCDIR_QT=/usr/local/bioinf/qt/include
QMAKE_LIBDIR_QT=/usr/local/bioinf/qt/lib
QMAKE_RTLDIR_QT=/usr/local/bioinf/qt/lib
OBJECTS_DIR=.obj/release-shared
MOC_DIR=.moc/release-shared
QT_INSTALL_PREFIX=/usr/local/bioinf/qt
docs.path=/usr/local/bioinf/qt/doc
headers.path=/usr/local/bioinf/qt/include
plugins.path=/usr/local/bioinf/qt/plugins
libs.path=/usr/local/bioinf/qt/lib
bins.path=/usr/local/bioinf/qt/bin
data.path=/usr/local/bioinf/qt
translations.path=/usr/local/bioinf/qt/translations
