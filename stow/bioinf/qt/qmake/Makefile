	-DQT_NO_COMPRESS -I/usr/local/bioinf/qt/mkspecs/linux-g++ -DHAVE_QCONFIG_CPP
LFLAGS=$(QMAKE_LFLAGS)

qmake: $(OBJS) $(QOBJS)
	$(CXX) -o $@ $(OBJS) $(QOBJS) $(LFLAGS)
	rm -f /usr/local/bioinf/qt/bin/$@
	ln -s ../qmake/$@ /usr/local/bioinf/qt/bin/$@

install: qmake
	[ -d $(INSTALL_ROOT)/usr/local/bioinf/qt/bin ] || mkdir -p $(INSTALL_ROOT)/usr/local/bioinf/qt/bin
	-cp -f /usr/local/bioinf/qt/bin/qmake $(INSTALL_ROOT)/usr/local/bioinf/qt/bin
	[ -d $(INSTALL_ROOT)/usr/local/bioinf/qt ] || mkdir -p $(INSTALL_ROOT)/usr/local/bioinf/qt
	-cp -R -f /usr/local/bioinf/qt/mkspecs $(INSTALL_ROOT)/usr/local/bioinf/qt
	-ln -s -f /usr/local/bioinf/qt/mkspecs/linux-g++ $(INSTALL_ROOT)/usr/local/bioinf/qt/mkspecs/default

clean::
	rm -f $(OBJS) $(QOBJS)

distclean:: clean
	rm -rf qmake .deps

depend:
	makedepend $(CXXFLAGS) -D__MAKEDEPEND__ $(DEPEND_SRC)

