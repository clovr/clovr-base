####### Compiler, tools and options

CC       = gcc
CXX      = g++
LEX      = flex
YACC     = yacc
CFLAGS   = -pipe -Wall -W -O2  -DQT_NO_DEBUG -DQT_SHARED
CXXFLAGS = -pipe -Wall -W -O2  -DQT_NO_DEBUG -DQT_SHARED
LEXFLAGS = 
YACCFLAGS= -d
INCPATH  = -I/usr/local/bioinf/qt/mkspecs/linux-g++ -I. -I../../../../../include -I.moc/release-shared/
LINK     = g++
LFLAGS   =  -Wl,-rpath,/usr/local/bioinf/qt/lib
LIBS     = $(SUBLIBS) -L/usr/local/bioinf/qt/lib -L/usr/X11R6/lib -lqt -lXext -lX11 -lm -lSM -lICE -ldl
AR       = ar cqs
RANLIB   = 
MOC      = /usr/local/bioinf/qt/bin/moc
UIC      = /usr/local/bioinf/qt/bin/uic -L /usr/local/bioinf/qt/plugins
QMAKE    = /usr/local/bioinf/qt/bin/qmake
TAR      = tar -cf
GZIP     = gzip -9f
COPY     = cp -f
COPY_FILE= $(COPY)
COPY_DIR = $(COPY) -r
INSTALL_FILE= $(COPY_FILE)
INSTALL_DIR = $(COPY_DIR)
DEL_FILE = rm -f
SYMLINK  = ln -sf
DEL_DIR  = rmdir
MOVE     = mv -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p

####### Output directory

OBJECTS_DIR = .obj/release-shared/

####### Files

HEADERS = 
SOURCES = main.cpp \
		../connection.cpp
OBJECTS = .obj/release-shared/main.o \
		.obj/release-shared/connection.o \
		.obj/release-shared/book.o \
		.obj/release-shared/editbook.o
FORMS = book.ui \
		editbook.ui
UICDECLS = book.h \
		editbook.h
UICIMPLS = book.cpp \
		editbook.cpp
SRCMOC   = .moc/release-shared/moc_book.cpp \
		.moc/release-shared/moc_editbook.cpp
OBJMOC = .obj/release-shared/moc_book.o \
		.obj/release-shared/moc_editbook.o
DIST	   = book5.pro
QMAKE_TARGET = book5
