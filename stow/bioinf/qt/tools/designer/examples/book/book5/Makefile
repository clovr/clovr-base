TARGET   = book5

first: all
####### Implicit rules

.SUFFIXES: .c .o .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd /usr/local/bioinf/qt/src/moc && $(MAKE) )

Makefile: book5.pro ../../../../../.qmake.cache /usr/local/bioinf/qt/mkspecs/linux-g++/qmake.conf ../../../../../lib/libqt.prl
	$(QMAKE) -spec /usr/local/bioinf/qt/mkspecs/linux-g++ -o Makefile book5.pro
qmake: 
	@$(QMAKE) -spec /usr/local/bioinf/qt/mkspecs/linux-g++ -o Makefile book5.pro

dist: 
	@mkdir -p .obj/release-shared/book5 && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/release-shared/book5/ && $(COPY_FILE) --parents book.ui.h editbook.ui.h .obj/release-shared/book5/ && ( cd `dirname .obj/release-shared/book5` && $(TAR) book5.tar book5 && $(GZIP) book5.tar ) && $(MOVE) `dirname .obj/release-shared/book5`/book5.tar.gz . && $(DEL_FILE) -r .obj/release-shared/book5

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)


FORCE:

####### Compile

.obj/release-shared/main.o: main.cpp book.h \
		../connection.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/main.o main.cpp

.obj/release-shared/connection.o: ../connection.cpp ../connection.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/connection.o ../connection.cpp

book.h: book.ui editbook.h \
		book.ui.h
	$(UIC) book.ui -o book.h

book.cpp: book.h book.ui book.ui.h editbook.h \
		book.ui.h
	$(UIC) book.ui -i book.h -o book.cpp

editbook.h: editbook.ui 
	$(UIC) editbook.ui -o editbook.h

editbook.cpp: editbook.h editbook.ui editbook.ui.h 
	$(UIC) editbook.ui -i editbook.h -o editbook.cpp

.obj/release-shared/book.o: book.cpp book.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/book.o book.cpp

.obj/release-shared/editbook.o: editbook.cpp editbook.ui.h \
		editbook.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/editbook.o editbook.cpp

.obj/release-shared/moc_book.o: .moc/release-shared/moc_book.cpp  book.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/moc_book.o .moc/release-shared/moc_book.cpp

.obj/release-shared/moc_editbook.o: .moc/release-shared/moc_editbook.cpp  editbook.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/moc_editbook.o .moc/release-shared/moc_editbook.cpp

.moc/release-shared/moc_book.cpp: $(MOC) book.h
	$(MOC) book.h -o .moc/release-shared/moc_book.cpp

.moc/release-shared/moc_editbook.cpp: $(MOC) editbook.h
	$(MOC) editbook.h -o .moc/release-shared/moc_editbook.cpp

