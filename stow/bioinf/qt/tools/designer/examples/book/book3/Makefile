CXX      = g++
LEX      = flex
YACC     = yacc
CFLAGS   = -pipe -Wall -W -O2  -DQT_NO_DEBUG -DQT_SHARED
CXXFLAGS = -pipe -Wall -W -O2  -DQT_NO_DEBUG -DQT_SHARED
LEXFLAGS = 
YACCFLAGS= -d
INCPATH  = -I/usr/local/bioinf/qt/mkspecs/linux-g++ -I. -I../../../../../include -I.moc/release-shared/
LINK     = g++
LFLAGS   =  -Wl,-rpath,/usr/local/bioinf/qt/lib
LIBS     = $(SUBLIBS) -L/usr/local/bioinf/qt/lib -L/usr/X11R6/lib -lqt -lXext -lX11 -lm -lSM -lICE -ldl
AR       = ar cqs
RANLIB   = 
MOC      = /usr/local/bioinf/qt/bin/moc
UIC      = /usr/local/bioinf/qt/bin/uic -L /usr/local/bioinf/qt/plugins
QMAKE    = /usr/local/bioinf/qt/bin/qmake
TAR      = tar -cf
GZIP     = gzip -9f
