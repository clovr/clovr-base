SOURCES = main.cpp \
		../connection.cpp
OBJECTS = .obj/release-shared/main.o \
		.obj/release-shared/connection.o \
		.obj/release-shared/book.o
FORMS = book.ui
UICDECLS = book.h
UICIMPLS = book.cpp
