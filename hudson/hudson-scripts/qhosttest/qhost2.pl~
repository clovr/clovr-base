#! /usr/bin/perl

$temp;
$expected=`cat expectedhosts`;

open (MYFILE, 'hosts');
while (<MYFILE>) {
    chomp;
    $temp=substr "$_",6; 
    $temp=~ tr/"//d;
    if ($expected =~ m/$temp/){
	print "match\n";
    }
    elsif($temp == "global"){}
    else{
	exit 1;
    }
   # print "$temp \n";
}
close (MYFILE); 

